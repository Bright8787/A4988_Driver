#include "stm32f446xx_Timer_driver.h"

void TIM_Init(Timer_Handle_t *pTIMHandle){





}


/*Enable Clock of the given Timer*/
void TIM_PeriClockControl(Timer_RegDef_t *pTIMx, uint8_t ENorDI){
//	assert(pGPIOx == NULL);
	if(ENorDI == ENABLE){

		if(pTIMx == pTIM2){
			TIM2_PCLK_EN()();
		}
		else if(pTIMx == pTIM3){
			TIM3_PCLK_EN()();
		}
		else if(pTIMx == pTIM4){
			TIM4_PCLK_EN()();
		}
		else if(pTIMx == pTIM5){
			TIM5_PCLK_EN()();
		}


	}
	else{
		if(pGPIOx == pGPIOA){
					GPIOA_PCLK_DI();
				}
				else if(pGPIOx == pGPIOB){
					GPIOB_PCLK_DI();

				}
				else if(pGPIOx == pGPIOC){
					GPIOC_PCLK_DI();

				}
				else if(pGPIOx == pGPIOD){
					GPIOD_PCLK_DI();

				}
				else if(pGPIOx == pGPIOE){
					GPIOE_PCLK_DI();

				}
				else if(pGPIOx == pGPIOF){
					GPIOF_PCLK_DI();

				}
				else if(pGPIOx == pGPIOG){

					GPIOG_PCLK_DI();
				}
				else if(pGPIOx == pGPIOH){

					GPIOH_PCLK_DI();
				}
	}
}
